<UserControl x:Class="Shane.Church.StirlingBirthday.Controls.BirthdayTileFront"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeSmall}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="173" d:DesignWidth="173">

	<Canvas Background="Transparent" Width="173" Height="173" x:Name="layoutRoot">
		<Image Source="{Binding Image}" Width="173" Height="173" Margin="0,0,0,0" />
		<Image Source="{Binding Overlay}" Width="173" Height="173" Margin="0,0,0,0" />
	</Canvas>
</UserControl>
